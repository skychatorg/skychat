    "types": "build/index.d.ts",
    "main": "build/index.js",
    "scripts": {
        "clean": "rm -rf dist build",
        "destroy": "bash app/script/destroy.sh",
        "setup": "bash app/script/setup.sh",
        "build:client": "vite build --emptyOutDir",
        "build:server": "PRODUCTION=true tsc",
        "build": "npm run build:client && npm run build:server",
        "dev:client": "vite build --emptyOutDir --watch",
        "dev:server": "PRODUCTION=false tsc-watch --onSuccess \"node build/server/server.js\"",
        "kill": "ps aux | grep 'node build/server/server.js' | awk '{print $2}' | xargs kill -9",
        "start": "npm run build && node build/server/server.js",
        "start:docker": "bash app/script/docker-start.sh",
        "backup": "bash app/script/backup.sh"
    },
